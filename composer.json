{
    "name": "njenga55/agora",
    "description": "Agora laravel package",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Njenga",
            "email": "njenga55@gmail.com"
        }
    ],
    "require": {
        "ramsey/uuid": "^4.1"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Njenga55\\Agora\\":"src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "njenga55\\Agora\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Njenga55\\Agora\\AgoraServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit tests/Feature"
        ]
    }
}
